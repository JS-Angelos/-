码" maxlength="11" cursor-spacing="{{22}}" data-event-opts="{{[['focus',[['focusFn',['$event']]]],['blur',[['blurFn',['$event']]]],['input',[['__set_model',['$0','phone','$event',[]],['form']]]]]}}" value="{{form.phone}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><view class="line"><text class="f-l-t">志愿中国账号：</text><input class="{{[[(focus==4)?'foc':''],[(err.$4==0)?'err0':''],[(err.$4==1)?'err1':''],'inp','age']}}" id="$4" type="idcard" placeholder="账号仅用于录入志愿时长" maxlength="18" cursor-spacing="{{22}}" data-event-opts="{{[['focus',[['focusFn',['$event']]]],['blur',[['blurFn',['$event']]]],['input',[['__set_model',['$0','idCard','$event',[]],['form']]]]]}}" value="{{form.idCard}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><view class="line"><text class="f-l-t">家庭住址：</text><view class="ip-shell"><picker class="picker" mode="region" value="{{form.regionArr}}" data-event-opts="{{[['change',[['addressChangeFn',['$event']]]]]}}" bindchange="__e"><view class="region"><text>{{form.regionArr[0]}}</text>-<text>{{form.regionArr[1]}}</text>-<text>{{form.regionArr[2]}}</text></view><text class="iconfont icon-xiangxia1"></text></picker><input class="{{[[(focus==5)?'foc':''],[(err.$5==0)?'err0':''],'inp']}}" id="$5" type="text" placeholder="请输入详细地址" cursor-spacing="{{22}}" data-event-opts="{{[['focus',[['focusFn',['$event']]]],['blur',[['blurFn',['$event']]]],['input',[['__set_model',['$0','address','$event',[]],['form']]]]]}}" value="{{form.address}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view></view><view class="line"><text class="f-l-t">政治面貌：</text><radio-group data-event-opts="{{[['change',[['changeFn',['$event']]]]]}}" class="radio-shell" bindchange="__e"><label class="radio"><radio value="0" checked="{{true}}" color="rgb(255,127,0)"></radio><text>群众</text></label><label class="radio"><radio value="1" color="rgb(0,87,55)"></radio><text>团员</text></label><label class="radio"><radio value="2" color="rgb(255,0,0)"></radio><text>党员</text></label></radio-group></view><view class="line"><text class="f-l-t">邮箱：</text><input class="{{[[(focus==6)?'foc':''],[(err.$6==1)?'err1':''],'inp','age']}}" id="$6" type="text" placeholder="非必填" cursor-spacing="{{22}}" maxlength="30" data-event-opts="{{[['focus',[['focusFn',['$event']]]],['blur',[['blurFn',['$event']]]],['input',[['__set_model',['$0','email','$event',[]],['form']]]]]}}" value="{{form.email}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><view class="line"><text class="f-l-t">单位/学校：</text><input class="{{[[(focus==7)?'foc':''],'inp']}}" id="$7" type="text" placeholder="二选一 , 非必填" maxlength="30" cursor-spacing="{{22}}" data-event-opts="{{[['focus',[['focusFn',['$event']]]],['blur',[['blurFn',['$event']]]],['input',[['__set_model',['$0','work','$event',[]],['form']]]]]}}" value="{{form.work}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><view class="line"><text class="f-l-t">隶属部门：</text><input class="{{[[(focus==8)?'foc':''],'inp']}}" id="$8" type="text" placeholder="非必填" maxlength="10" cursor-spacing="{{22}}" data-event-opts="{{[['focus',[['focusFn',['$event']]]],['blur',[['blurFn',['$event']]]],['input',[['__set_model',['$0','department','$event',[]],['form']]]]]}}" value="{{form.department}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><view class="auth_text"><block wx:if="{{!ifAuth}}"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="iconfont icon-square" bindtap="__e"></text></block><block wx:else><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="iconfont icon-squarecheckfill" style="color:#1ed76d;" bindtap="__e"></text></block><view class="auth">确认授权<text style="color:#dd6572;font-weight:bold;">梓云志愿者</text>小程序收集您的<text style="color:#dd6572;font-weight:bold;">个人信息</text></view></view><button class="btn" style="{{'background-color:'+(ifAuth?'#428bca':'#f7f7f7')+';'+('color:'+(ifAuth?'#FFF':'#989898')+';')}}" disabled="{{!ifAuth}}" data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" bindtap="__e