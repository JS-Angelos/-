.add-userinfo {
  padding: 20px 0;
}
.add-userinfo .title {
  padding: 10px 0;
  font-size: 20px;
  text-align: center;
  font-weight: 600;
  line-height: 30px;
  color: #333;
}
.add-userinfo .content {
  font: 400 13px/1.5 "Hiragino Sans GB", "Microsoft Yahei", arial, sans-serif;
  color: #333;
}
.add-userinfo .content .ps-shell {
  margin-bottom: 10px;
  border-bottom: 1px dashed #DEDEDE;
  padding: 10px 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.add-userinfo .content .ps-shell .ps {
  font-size: 11px;
  line-height: 22px;
  font-weight: 400;
  color: #959595;
}
.add-userinfo .content .form .line {
  width: 100%;
  padding: 20px 25px;
  border-bottom: 1px dashed #DFDFDF;
  font-size: 15px;
  color: #808080;
  line-height: 20px;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.add-userinfo .content .form .line .f-l-t {
  display: block;
  font-size: 15px;
  margin-right: 10px;
  font-weight: 400;
  line-height: 20px;
}
.add-userinfo .content .form .line .inp {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 20px;
  margin-bottom: 2px;
  padding: 5px;
  font-family: inherit;
  font-size: 13px;
  line-height: 20px;
  border-radius: 4px;
  color: #555;
  border: 1px solid #ccc;
  outline: none;
  box-shadow: none;
  -webkit-transition: border linear 0.2s, box-shadow linear 0.2s;
  transition: border linear 0.2s, box-shadow linear 0.2s;
}
.add-userinfo .content .form .line .inp.foc {
  border-color: rgba(82, 168, 236, 0.8);
  box-shadow: inset 0 1px 1px rgb(0 0 0%), 0 0 8px rgb(82 168 3.93333333%);
}
.add-userinfo .content .form .line .inp.err0 {
  position: relative;
  border-color: #b94a48;
  box-shadow: none;
}
.add-userinfo .content .form .line .inp.err0::after {
  content: "必填";
  position: absolute;
  right: 10px;
  top: 6px;
  padding-left: 10px;
  background: inherit;
  color: #C00;
  font-size: 12px;
  z-index: 1;
  font-weight: bolder;
}
.add-userinfo .content .form .line .inp.err1 {
  position: relative;
  border-color: #b94a48;
  box-shadow: none;
}
.add-userinfo .content .form .line .inp.err1::after {
  content: "格式错误";
  position: absolute;
  right: 10px;
  top: 6px;
  padding-left: 10px;
  background: inherit;
  color: #C00;
  font-size: 12px;
  z-index: 1;
  font-weight: bolder;
}
.add-userinfo .content .form .line .sex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-left: 25px;
}
.add-userinfo .content .form .line .sex .radio {
  position: relative;
  width: 122px;
  height: 42px;
}
.add-userinfo .content .form .line .sex .radio .txt-shell {
  width: calc(100% - 2px);
  height: calc(100% - 2px);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  border: 1px solid #ccc;
  border-radius: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.add-userinfo .content .form .line .sex .radio .txt-shell .txt {
  display: inline-block;
  width: 50%;
  height: 100%;
  line-height: 40px;
  text-align: center;
  -webkit-transition: color 0.3s;
  transition: color 0.3s;
}
.add-userinfo .content .form .line .sex .radio .txt-shell .txt.on {
  color: #fff;
}
.add-userinfo .content .form .line .sex .radio .sliding {
  position: absolute;
  top: 1px;
  left: 0;
  width: calc(50% - 1px);
  height: calc(100% - 2px);
  margin-left: 0;
  border-radius: 10px;
  background-color: #5aa7f9;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
}
.add-userinfo .content .form .line .sex .radio .sliding.girl {
  margin-left: calc(50% + 1px);
  background-color: #ff9dc6;
}
.add-userinfo .content .form .line .age-shell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.add-userinfo .content .form .line .age-shell .age {
  padding-left: 20px;
  width: 40px;
}
.add-userinfo .content .form .line .radio-shell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
}
.add-userinfo .content .form .line .radio-shell .radio {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.add-userinfo .content .form .line .radio-shell .radio + .radio {
  margin-left: 20px;
}
.add-userinfo .content .form .line .ip-shell {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.add-userinfo .content .form .line .ip-shell .picker {
  position: relative;
  border-radius: 4px;
  padding: 7px 5px;
  color: #555;
  border: 1px solid #ccc;
  margin-bottom: 15px;
}
.add-userinfo .content .form .line .ip-shell .picker .region {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 100%;
  height: 100%;
  padding-right: 40px;
  box-sizing: border-box;
  font-family: inherit;
  font-size: 13px;
  line-height: 20px;
}
.add-userinfo .content .form .line .ip-shell .picker .iconfont {
  position: absolute;
  right: 15px;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.add-userinfo .content .form .auth_text {
  padding: 20px 25px 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 12px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.add-userinfo .content .form .auth_text .iconfont {
  margin: 2px 3px 0 0;
  font-size: 13px;
}
.add-userinfo .content .form .btn {
  margin: 30px 15px 20px;
  color: #fff;
  background: #428bca;
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
}
.add-userinfo .content .form .btn:active {
  background-color: #286090;
  box-shadow: inset 0 2px 4px rgb(0 0 0%), 0 1px 2px rgb(0 0 0%);
}

