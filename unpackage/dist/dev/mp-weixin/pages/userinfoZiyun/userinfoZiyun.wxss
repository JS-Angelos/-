.warp {
  height: 800px;
  background-color: #f8f7f9;
}
.warp .title {
  padding: 20px 10px 10px;
  font-weight: bold;
  font-size: 30px;
  color: #444444;
  text-align: center;
}
.warp .ps-shell {
  border-bottom: 1px solid #DEDEDE;
  padding: 10px 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.warp .ps-shell .ps {
  position: relative;
  padding-left: 8px;
  font-size: 11px;
  line-height: 18px;
  font-weight: 400;
  color: #959595;
}
.warp .ps-shell .ps::before {
  position: absolute;
  content: "*";
  left: 0;
  top: 2px;
}
.warp .data-shell {
  width: 100%;
  margin: 0 auto;
}
.warp .data-shell .data {
  width: 100%;
  background-color: #fff;
}
.warp .data-shell .data + .data {
  margin-top: 8px;
}
.warp .data-shell .data .line {
  padding: 15px 20px;
  border-bottom: 1px solid #ebebeb;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.warp .data-shell .data .line .left {
  color: #646464;
}

